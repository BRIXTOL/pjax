{
  "npm.packageManager": "pnpm",
  "eslint.packageManager": "pnpm",
  "[typescript]": {
    "editor.parameterHints.enabled": true,
    "editor.suggest.showModules": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "cSpell.words": [
    "spx",
    "navigations",
    "Alexey",
    "hrefs",
    "Спасибі"
  ]
}
