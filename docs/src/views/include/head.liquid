<title>
  {{ title | prepend: 'SPX | ' }}
</title>

<meta charset="UTF-8">
<meta
  name="description"
  content="{{ description }}">
<meta
  http-equiv="X-UA-Compatible"
  content="IE=edge">
<meta
  name="viewport"
  content="width=device-width, initial-scale=1.0">
<link
  href="https://fonts.googleapis.com"
  rel="preconnect">
<link
  href="https://fonts.gstatic.com"
  crossorigin
  rel="preconnect">
<link
  href="https://fonts.googleapis.com/css2?family=JetBrains+Mono&display=swap"
  rel="stylesheet">

<link
  type="font/woff2"
  href="{{ '/assets/fonts/proxima-nova-regular.woff2' | url }}"
  as="font"
  crossorigin
  rel="stylesheet preload" />

<link
  type="font/woff2"
  href="{{ '/assets/fonts/proxima-nova-bold.woff2' | url }}"
  as="font"
  crossorigin
  rel="stylesheet preload" />

<link
  type="font/woff2"
  href="{{ '/assets/fonts/proxima-nova-light.woff2' | url }}"
  as="font"
  crossorigin
  rel="stylesheet preload" />

<style spx-eval="false">
  @font-face {
    font-family: 'Proxima Nova';
    src: url('{{ '/assets/fonts/proxima-nova-regular.woff2' | url }}') format('woff2');
    font-weight: 300;
    font-style: normal;
    font-display: swap;
  }

  @font-face {
    font-family: 'Proxima Nova';
    src: url('{{ '/assets/fonts/proxima-nova-bold.woff2' | url }}') format('woff2');
    font-weight: 600;
    font-style: normal;
    font-display: swap;
  }

  @font-face {
    font-family: 'Proxima Nova';
    src: url('{{ '/assets/fonts/proxima-nova-light.woff2' | url }}') format('woff2');
    font-weight: 100;
    font-style: normal;
    font-display: swap;
  }
</style>

<link
  href="{{'/style.css' | url }}"
  rel="stylesheet"
  spx-eval="false">

{%- if iframe -%}
  <script
    src="{{ '/iframe.min.js' | url }}"
    spx-eval="false"
    defer></script>
{%- else -%}
  <script
    src="{{'/bundle.min.js' | url }}"
    spx-eval="false"
    async></script>
{%- endif -%}